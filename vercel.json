{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "buildCommand": "npm run build" }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "s-maxage=31536000, immutable" },
      "dest": "build/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "build/server/index.js"
    }
  ]
}